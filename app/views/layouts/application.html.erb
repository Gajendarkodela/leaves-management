<!DOCTYPE html>
<html>
  <head>
    <title>Projects Management</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="fixed-nav sticky-footer <%=current_user ? '' : 'bg-promo'%>">
    <% if current_user %>
  	 <%= render partial: 'layouts/header'%>
    <% end %>

    <div class="content-wrapper" id="main_body">
      <div class="renderer">
        
      </div>
      <div class="append_test_cases">
      </div>
      <div class="container">
        <%if flash[:notice].present?%>
          <div class="alert alert-success" id="alert-success" role="alert">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
               <span aria-hidden="true">&times;</span>
            </button>
            <div id="success-message"><%= flash[:notice]%></div>
          </div>
        <%end%>
        <div class="alert alert-success dis-n" id="alert-success" role="alert">
          <button type="button" class="close" data-dismiss="alert" aria-label="Close">
             <span aria-hidden="true">&times;</span>
          </button>
          <div id="success-message2"><%= flash[:notice]%></div>
        </div>
      	<%= yield %>
      </div>
    </div>  
  </body>
</html>
