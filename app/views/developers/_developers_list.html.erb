<div id="projects_list">
	<table class="table table-bordered">
	 	<thead>
	 		<tr>
	 			<td></td>
	 			<td>Name</td>
	 			<td>Email</td>
	 		</tr>
	 	</thead>
	  <tbody>
			<% developers.each_with_index do |dev, index| %>
			 <tr>
				 <td> <%= index+1 %></td>
				 <td><%= link_to dev.name, edit_developer_path(dev), remote: true %></td>
				 <td> <%= dev.email%></td>
		   </tr>
			<% end %>
		</tbody>
	</table>	
</div>